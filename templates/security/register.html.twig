<!-- begin::Body -->
<body  class="kt-quick-panel--right kt-demo-panel--right kt-offcanvas-panel--right kt-header--fixed kt-header-mobile--fixed kt-subheader--enabled kt-subheader--fixed kt-subheader--solid kt-aside--enabled kt-aside--fixed kt-page--loading"  >

<!-- begin:: Page -->
{% block body %}
<div class="kt-grid kt-grid--ver kt-grid--root">
    <div class="kt-grid kt-grid--hor kt-grid--root  kt-login kt-login--v3 kt-login--signin" id="kt_login">
        <div class="kt-grid__item kt-grid__item--fluid kt-grid kt-grid--hor" style="background-image: url({{ asset("media/bg/bg-3.jpg") }});">
            <div class="kt-grid__item kt-grid__item--fluid kt-login__wrapper" style="padding-top: 35px;">
                <div class="kt-login__container">
                    <div class="kt-login__logo">
                        <a href="#">
                            <img src="{{ asset("media/logos/logo-9.png") }}">
                        </a>
                    </div>
                    <div class="kt-login__signin">
                        <div class="kt-login__head" style="margin-bottom: 1rem;">
                            <h3 class="kt-login__title">Créer un nouveau compte </h3>
                            <div class="kt-login__desc">Entrer les informations relatifs à votre compte </div>
                        </div>
                        {{ form_start(registrationForm, {'attr': {'class': 'kt-form'}}) }}
                            <div class="input-group">
                                {{ form_widget(registrationForm.username) }}
                            </div>
                            <div class="input-group">
                                {{ form_widget(registrationForm.email ) }}
                                {{ form_errors(registrationForm.email ) }}
                            </div>
                            <div class="input-group">
                                {{ form_widget(registrationForm.plainPassword.first )}}
                            </div>
                            <div class="input-group">
                                {{ form_widget(registrationForm.plainPassword.second )}}
                            </div>
                            <div class="input-group" style="align-items: baseline; ">
                                {{ form_label(registrationForm.personne) }} {{ form_widget(registrationForm.personne) }}
                            </div>
                            <div class="row kt-login__extra">
                                <div class="col kt-align-left">
                                    {{ form_row( registrationForm.agreeTerms ) }}
                                    <span class="form-text text-muted"></span>
                                </div>
                            </div>
                            <div class="kt-login__actions">
                                <button id="kt_login_signup_submit" class="btn btn-brand btn-elevate kt-login__btn-primary">Créer votre compte</button>&nbsp;&nbsp;
                            </div>
                        {{ form_end(registrationForm) }}
                    </div>
                    <div class="kt-login__account">
                        <span class="kt-login__account-msg">
                            Vous avez déjà un compte ?
                        </span>
                        &nbsp;&nbsp;
                        <a href="/login" id="kt_login_signup" class="kt-login__account-link">Connectez vous</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- end:: Page -->
{% endblock %}

